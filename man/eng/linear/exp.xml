    <SEE_ALSO_ITEM>
      <LINK>coff</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>log</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>expm</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>
