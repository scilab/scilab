<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>sinh</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>April 1993</DATE>
  <SHORT_DESCRIPTION name="sinh"> hyperbolic sine</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>[t]=sinh(x)   </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>x,t</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: real or complex vectors/matrices</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P>
    the elements   of   vector  <VERB>t</VERB>  are the   
    hyperbolic  sine  of elements of vector <VERB>x</VERB>.</P>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
asinh(sinh([0,1,%i]))
 ]]>
  </EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>asinh</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>
