<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>date</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>Jun 1999</DATE>
  <SHORT_DESCRIPTION name="date"> Current date as date string</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>dt=date()  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>dt</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: a string</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P>
    dt=date() returns a string containing the date in dd-mmm-yyyy format.</P>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
date()
 ]]>
  </EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>getdate</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>timer</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>
