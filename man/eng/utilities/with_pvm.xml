<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
	<LANGUAGE>eng</LANGUAGE>
	<TITLE>with_pvm</TITLE>
	<TYPE>Scilab Function</TYPE>
	<DATE>2002</DATE>
	<SHORT_DESCRIPTION name="with_pvm"> Checks if Scilab has been built with the &quot;Parallel Virtual Machine&quot; interface</SHORT_DESCRIPTION>
	<CALLING_SEQUENCE>
		<CALLING_SEQUENCE_ITEM>r=with_pvm()</CALLING_SEQUENCE_ITEM>
	</CALLING_SEQUENCE>
	<PARAM>
		<PARAM_INDENT>
			<PARAM_ITEM>
				<PARAM_NAME>r</PARAM_NAME>
				<PARAM_DESCRIPTION>
					<SP>: a boolean</SP>
				</PARAM_DESCRIPTION>
			</PARAM_ITEM>
		</PARAM_INDENT>
	</PARAM>
	<DESCRIPTION>
		<P>Returns <VERB>%t</VERB> if Scilab as been built  with the &quot;Parallel Virtual Machine&quot; interface or <VERB>%f</VERB> if not.</P>
	</DESCRIPTION>
</MAN>
