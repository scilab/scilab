<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>pwd</TITLE>
  <TYPE>Scilab command</TYPE>
  <DATE>April 1998</DATE>
  <SHORT_DESCRIPTION name="pwd"> print Scilab current directory</SHORT_DESCRIPTION>
  <SHORT_DESCRIPTION name="getcwd"> get Scilab current directory</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>pwd</CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>x=pwd()  </CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>x=getcwd()  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <DESCRIPTION>
    <P><VERB>pwd</VERB> returns in <VERB>ans</VERB> the Scilab current directory.
    <VERB>x=pwd()</VERB> or <VERB>x=getcwd()</VERB> returns in <VERB>x</VERB> the Scilab current
    directory.</P>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
pwd
x=pwd()
 ]]>
  </EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>chdir</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>cd</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>
