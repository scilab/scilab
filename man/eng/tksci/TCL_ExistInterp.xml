<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>TCL_ExistInterp</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>18 November 2005</DATE>

  <SHORT_DESCRIPTION name="TCL_ExistInterp">Return %T if a tcl slave interperter
  exists</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>OK=TCL_ExistInterp(interp)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>interp</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: character string parameter. Name of the slave tcl
          interpreter.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>

      <PARAM_ITEM>
        <PARAM_NAME>ok</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: boolean. %T if TCL interpreter exists.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>This routine allows to test if TCL interpreter exists.</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[
  TCL_ExistInterp('SlaveInterp')
  TCL_CreateSlave('SlaveInterp')
  TCL_ExistInterp('SlaveInterp')
  TCL_DeleteInterp('SlaveInterp')]]></EXAMPLE>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>TCL_CreateSlave</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>TCL_DeleteInterp</LINK>
    </SEE_ALSO_ITEM>
</SEE_ALSO>
  <AUTHOR>Allan CORNET</AUTHOR>
</MAN>