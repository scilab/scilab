<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>fr</LANGUAGE>

  <TITLE>findmsvccompiler</TITLE>

  <TYPE>Fonction Scilab</TYPE>

  <DATE>28-Fev-2005</DATE>

  <SHORT_DESCRIPTION name="findmsvccompiler">retourne la version du
  compilateur Microsoft C detecté</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>msvc=findmsvccompiler()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>msvc</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: retourne 'msvc50','msvc60','msvc70','msvc71','unknown'</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>retourne la version du compilateur Microsoft C detecté.</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[msvc=findmsvccompiler()
]]></EXAMPLE>

  <AUTHOR>Allan CORNET</AUTHOR>
</MAN>