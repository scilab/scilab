# The user wants to compile the localization files
# Please note that ALL_LINGUAS is defined in configure.ac
@GENERATE_LOCALIZATION_FILES_TRUE@localization:
@GENERATE_LOCALIZATION_FILES_TRUE@	@echo "-- Build localization (.mo from .po) --"; \
@GENERATE_LOCALIZATION_FILES_TRUE@	MASK="modules/*/locales/"; \
@GENERATE_LOCALIZATION_FILES_TRUE@	LC="LC_MESSAGES"; \
@GENERATE_LOCALIZATION_FILES_TRUE@	MSGFMT_OPTS="$MSGFMT_OPTS --check"; \
@GENERATE_LOCALIZATION_FILES_TRUE@	EXTENSION="*.po"; \
@GENERATE_LOCALIZATION_FILES_TRUE@	rm modules/*/locales/*~ 2> /dev/null; \
@GENERATE_LOCALIZATION_FILES_TRUE@	for LOCALE in $(ALL_LINGUAS); do \
@GENERATE_LOCALIZATION_FILES_TRUE@		echo "        -- Building for $$LOCALE --"; \
@GENERATE_LOCALIZATION_FILES_TRUE@		if test "$$LOCALE" == "en_US"; then \
@GENERATE_LOCALIZATION_FILES_TRUE@			LOCFILE=$$MASK/*.pot; \
@GENERATE_LOCALIZATION_FILES_TRUE@		else \
@GENERATE_LOCALIZATION_FILES_TRUE@			LOCFILE=$$MASK/$$LOCALE$$EXTENSION; \
@GENERATE_LOCALIZATION_FILES_TRUE@		fi; \
@GENERATE_LOCALIZATION_FILES_TRUE@		FILES=`ls $$LOCFILE`; \
@GENERATE_LOCALIZATION_FILES_TRUE@		PATHTO=$(top_builddir)/locale/$$LOCALE/$$LC/; \
@GENERATE_LOCALIZATION_FILES_TRUE@		if test ! -d $$PATHTO; then \
@GENERATE_LOCALIZATION_FILES_TRUE@			echo "        Creating $$PATHTO"; \
@GENERATE_LOCALIZATION_FILES_TRUE@			mkdir -p $$PATHTO; \
@GENERATE_LOCALIZATION_FILES_TRUE@		fi; \
@GENERATE_LOCALIZATION_FILES_TRUE@		POFILE=$$PATHTO/$(LANG_DOMAIN).po; \
@GENERATE_LOCALIZATION_FILES_TRUE@		$(MSGCAT) --use-first -o $$POFILE $$FILES || exit 2; \
@GENERATE_LOCALIZATION_FILES_TRUE@		if test "$(MACOSX)" = "1"; then sed -i -e '/charset=UTF-8/d' $$POFILE; fi; \
@GENERATE_LOCALIZATION_FILES_TRUE@		$(MSGFMT) $(MSGFMT_OPTS) --statistics -o $$PATHTO/$(LANG_DOMAIN).mo $$POFILE || exit 2; \
@GENERATE_LOCALIZATION_FILES_TRUE@	done;

@GENERATE_LOCALIZATION_FILES_TRUE@localization-status:
