#, c-format
msgid "%s: Wrong number of input arguments: %d expected.\n"
msgstr "%s: Неверное количество входных аргументов: ожидалось %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A matrix of strings expected.\n"
msgstr "%s: Неверный тип входного аргумента №%d: ожидалась матрица строк.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A scalar string expected.\n"
msgstr "%s: Неверный тип входного аргумента №%d: ожидалась скалярная строка.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A two-columns string matrix "
"expected.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: ожидалась двухстолбцовая строковая "
"матрица.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d or %d expected.\n"
msgstr "%s: Неверное количество входных аргументов: ожидалось %d или %d.\n"

#, c-format
msgid "%s: No active coverage: can't write.\n"
msgstr "%s: Нет активного перекрытия: не могу записать.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: html output only.\n"
msgstr "%s: Неверное значение входного аргумента №%d: только html-выход.\n"
