#, c-format
msgid "%s: Wrong number of input argument(s): %d to %d expected.\n"
msgstr ""
"%s: Nombre incorrecte d'arguments d'entrada: Han de ser entre %d i %d.\n"

#, c-format
msgid "%s: Wrong number of output argument(s): %d expected.\n"
msgstr "%s: Nombre d'arguments de sortida incorrecte: Hi ha d'haver %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A String expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d és d'un tipus incorrecte: Ha de ser una cadena "
"de text.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A Boolean expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d és d'un tipus incorrecte: Ha de ser un booleà.\n"

#, c-format
msgid "%s: Slave interpreter already exists.\n"
msgstr "%s: L'interpretador esclau ja existeix.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Real or String matrix expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d és d'un tipus incorrecte: Ha de ser una matriu "
"real o de cadenes de text.\n"

#, c-format
msgid "%s: No such slave interpreter.\n"
msgstr "%s: No existeix aquest interpretador esclau.\n"

msgid "Updating, please wait"
msgstr "S'està actualitzant, espereu"

#, c-format
msgid "Error %s while loading library %s"
msgstr "Error %s en llegir la llibreria %s"

msgid "Tcl/Tk interface not loaded."
msgstr "No s'ha carregat la interfície Tcl/Tk."

msgid "Open"
msgstr "Obre"

msgid "Warning: Problem(s) with TCL/TK interface. Interface not enabled.\n"
msgstr ""
"Avís: Hi ha hagut problemes amb la interfície TCL/TK, perquè no està "
"habilitada.\n"

msgid "Tcl/TK interface disabled in -nogui mode.\n"
msgstr "La interfície Tcl/TK està inhabilitada en el mode -nogui.\n"

#, c-format
msgid "%s: Error main TCL interpreter not initialized.\n"
msgstr "%s: Error. L'interpretador principal del TCL no està inicialitzat.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d és d'un tipus incorrecte: Ha de ser un text.\n"

#, c-format
msgid "%s: File %s not found.\n"
msgstr "%s: No s'ha trobat el fitxer %s.\n"

#, c-format
msgid ""
"%s, at line %i of file %s\n"
"\t%s.\n"
msgstr ""
"%s, a la línia %i del fitxer %s\n"
"\t%s.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: String or vector of strings "
"expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d és d'un tipus incorrecte: Ha de ser una cadena "
"de text o un vector de cadenes de text.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s: No hi ha més memòria.\n"

#, c-format
msgid "%s: Could not read Tcl Variable.\n"
msgstr "%s: No s'ha pogut llegir la variable Tcl.\n"

msgid "Alpha Release"
msgstr "Versió alfa"

msgid "Beta Release"
msgstr "Versió beta"

msgid "Final Release"
msgstr "Versió final"

msgid "Unknown Release"
msgstr "Versió desconeguda"

#, c-format
msgid "%s: Wrong value for input argument #%d: '%s' expected.\n"
msgstr "%s: Wrong value for input argument #%d: '%s' expected.\n"

msgid "This function doesn't work with Complex.\n"
msgstr "Aquesta funció no funciona amb nombres complexos.\n"

msgid "[] doesn't work with Tcl/Tk.\n"
msgstr "[] no funciona amb Tcl/Tk.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Matrix expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d és d'un tipus incorrecte: Ha de ser una "
"matriu.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d or #%d: String expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d o #%d és d'un tipus incorrecte: Ha de ser una "
"cadena de text.\n"

#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: Hi ha hagut un error: %s\n"

msgid "Impossible to set environment variable."
msgstr "No s'ha pogut definir la variable d'entorn."

msgid "Impossible to set system encoding."
msgstr "No s'ha pogut establir la codificació del sistema."

msgid ""
"The SCI environment variable is not set.\n"
"TCL initialisation failed !\n"
msgstr ""
"No s'ha definit la variable d'entorn SCI.\n"
"Ha fallat la inicialització del Tcl.\n"

msgid ""
"Unable to find Tcl initialisation scripts.\n"
"Check your SCI environment variable.\n"
"Tcl initialisation failed !"
msgstr ""
"No s'ha pogut trobar els script d'inicialització del Tcl.\n"
"Comproveu les variables d'entorn del SCI.\n"
"Ha fallat la inicialització del Tcl."

msgid ""
"The SCI environment variable is not set.\n"
"Tcl initialisation failed !\n"
msgstr ""
"No s'ha definit la variable d'entorn SCI.\n"
"Ha fallat la inicialització del Tcl.\n"

msgid "Tcl Error: Unable to create Tcl interpreter (Tcl_CreateInterp).\n"
msgstr ""
"Error del Tcl: No s'ha pogut crear l'intèrpret del Tcl (Tcl_CreateInterp).\n"

#, c-format
msgid "Tcl Error: Error during the Tcl initialization (Tcl_Init): %s\n"
msgstr ""
"Error del Tcl: S'ha produït un error a la inicialització del Tcl (Tcl_Init): "
"%s\n"

#, c-format
msgid "Tcl Error: Error during the TK initialization (Tk_Init): %s\n"
msgstr ""
"Error del Tcl: S'ha produït un error a la inicialització del TK (Tk_Init): "
"%s\n"

#, c-format
msgid ""
"Tcl Error: Error during the Scilab/Tcl init process. Could not set SciPath: "
"%s\n"
msgstr ""
"Error del Tcl: S'ha produït un error al procés d'inici del Scilab/Tcl. No "
"s'ha pogut definir el SciPath: %s\n"

#, c-format
msgid ""
"Tcl Error: Error during the Scilab/TK init process. Error while loading %s: "
"%s\n"
msgstr ""
"Error del Tcl: S'ha produït un error al procés d'inici del Scilab/Tcl. Error "
"en la càrrega del %s: %s\n"

#, c-format
msgid "TCL_EvalScilabCmd %s"
msgstr "TCL_EvalScilabCmd %s"

#, c-format
msgid "Execution starts for %s"
msgstr "L'execució comença per %s"

#, c-format
msgid "Execution ends for %s"
msgstr "L'execució acaba per %s"

msgid " Flushing starts for queued commands.\n"
msgstr ""

msgid "Warning: Too many callbacks in queue!\n"
msgstr "Avís: Hi ha massa crides encuades!\n"

