msgid "%s: Wrong number of input argument(s): at least one expected.\n"
msgstr ""
"%s: El nombre d'arguments d'entrada és incorrecte: N'hi ha d'haver un com a "
"mínim.\n"

#, c-format
msgid "Tcl Error : %s\n"
msgstr "Error del Tcl: %s\n"

#, c-format
msgid "Tcl Error: %s\n"
msgstr "Error del Tcl: %s\n"

msgid "Variable too long.\n"
msgstr "La variable és massa llarga.\n"

#, c-format
msgid "%s: Error TCL interpreter not initialized.\n"
msgstr "%s: Error, l'intèrpret del Tcl no s'ha inicialitzat.\n"

