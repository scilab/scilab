msgid " Flushing starts for queued commands.\n"
msgstr " Flushing começa para os comandos em fila.\n"

#, c-format
msgid ""
"%s, at line %i of file %s\n"
"\t%s.\n"
msgstr ""
"%s, na linha %i do arquivo %s\n"
"\t%s.\n"

#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: Ocorreu um erro: %s\n"

