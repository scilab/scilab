msgid "%s: Wrong type for input argument #%d: A Boolean expected.\n"
msgstr ""
"%s: Tipo errado para o argumento de entrada #%d: Esperava-se um booleano.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A String expected.\n"
msgstr ""
"%s: Tipo errado para o argumento de entrada #%d: Esperava-se um string.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Matrix expected.\n"
msgstr ""
"%s: Tipo errado para o argumento de entrada #%d: Esperava-se uma matriz.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr ""
"%s: Tipo errado para o argumento de entrada #%d: Esperava-se um string.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: String or vector of strings "
"expected.\n"
msgstr ""
"%s: Tipo errado para o argumento de entrada #%d: Esperava-se um string ou um "
"vetor de strings.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: '%s' expected.\n"
msgstr "%s: Valor errado para argumento de entrada #%d: Esperava-se '%s'.\n"

