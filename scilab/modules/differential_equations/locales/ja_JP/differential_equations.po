#
# File: macros/odeoptions.sci, line: 50
msgid "Defining %ODEOPTIONS variable"
msgstr "%ODEOPTIONS 変数の定義"

#
# File: macros/odeoptions.sci, line: 52
msgid "Meaning of itask and tcrit:"
msgstr "itaskとtcritの意味:"

#
# File: macros/odeoptions.sci, line: 53
msgid "1 : normal computation at specified times"
msgstr "1 : 普通の計算指定された時間"

#
# File: macros/odeoptions.sci, line: 54
msgid "2 : computation at mesh points (given in first row of output of ode)"
msgstr "2:  メッシュ点の計算(常微分方程式の出力の最初の列で与えられる)"

#
# File: macros/odeoptions.sci, line: 55
msgid "3 : one step at one internal mesh point and return"
msgstr "3:  １つの内部のメッシュ点のワンステップと戻り"

#
# File: macros/odeoptions.sci, line: 56
msgid "4 : normal computation without overshooting tcrit"
msgstr "4 : tcritを行き過ぎることのない普通の計算"

#
# File: macros/odeoptions.sci, line: 57
msgid "5 : one step, without passing tcrit, and return"
msgstr "5 : ワンステップ、tcritを通過すぎないで、そして戻り"

#
# File: macros/odeoptions.sci, line: 59
msgid "Meaning of jactype:"
msgstr "jactype の意味:"

#
# File: macros/odeoptions.sci, line: 60
msgid "0 : functional iterations (no jacobian used (''adams'' or ''stiff'' only))"
msgstr "0 : 機能的な繰り返し (ヤコビアンは使われません(''adams'' または ''stiff'' のみ))"

#
# File: macros/odeoptions.sci, line: 61
msgid "1 : user-supplied full jacobian"
msgstr "1 :  ユーザー満たされた 最大限のヤコビアン"

#
# File: macros/odeoptions.sci, line: 62
msgid "2 : internally generated full jacobian"
msgstr "2 : 内部で生成 最大限のヤコビアン"

#
# File: macros/odeoptions.sci, line: 63
msgid "3 : internally generated diagonal jacobian (''adams'' or ''stiff'' only)"
msgstr "3 : 内部で生成された対角線のヤコビアン (''adams'' または ''stiff'' のみ)"

#
# File: macros/odeoptions.sci, line: 64
msgid "4 : user-supplied banded jacobian (see ml,mu)"
msgstr "4 : ユーザーを満たす ヤコビアン帯域（ml,muを参照)"

#
# File: macros/odeoptions.sci, line: 65
msgid "5 : internally generated banded jacobian (see ml,mu)"
msgstr "5 :  内部で生成 ヤコビアン帯域（ml,muを参照)"

#
# File: macros/odeoptions.sci, line: 67
msgid "Meaning of ml,mu:"
msgstr "ml,mu の意味:"

#
# File: macros/odeoptions.sci, line: 68
msgid "If jactype = 4 or 5 ml and mu are the lower and upper half-banwidths"
msgstr "もし、 jactype = 4 または 5 ml と mu では、半-帯域は上下します"

#
# File: macros/odeoptions.sci, line: 69
msgid "of the banded jacobian: the band is the i,j''s with i-ml <= j <= ny-1"
msgstr "ヤコビアン帯域の: 帯域は、i,j で i-ml <= j <= ny-1"

#
# File: macros/odeoptions.sci, line: 70
msgid "If jactype = 4 the jacobian function must return"
msgstr "もし、jactype = 4 であるなら、ヤコビアン関数は戻らなくてはなりません"

#
# File: macros/odeoptions.sci, line: 71
msgid "a matrix J which is  ml+mu+1 x ny (where ny=dim of y in ydot=f(t,y))"
msgstr "ml+mu+1 x ny である 行列 J (ydot=f(t,y) で y の ny=dim のどこか)"

#
# File: macros/odeoptions.sci, line: 72
msgid "such that column 1 of J is made of mu zeros followed by"
msgstr "Jの列１がゼロのmuで作られている"

#
# File: macros/odeoptions.sci, line: 73
msgid "df1/dy1, df2/dy1, df3/dy1,... (1+ml possibly non-zero entries)"
msgstr "df1/dy1, df2/dy1, df3/dy1,... (1+ml おそらくはゼロでない入力)"

#
# File: macros/odeoptions.sci, line: 74
msgid "column 2 is made of mu-1 zeros followed by df1/dx2, df2/dx2,etc"
msgstr "列２は、ゼロが df1/dx2, df2/dx2,etc によってmu-1で作られています"

#
# File: macros/odeoptions.sci, line: 76
msgid "Default values are given in square brackets"
msgstr "デフォルト値が角括弧で与えられます"

#
# File: macros/odeoptions.sci, line: 77
msgid "If the function is called without argument, default values are used"
msgstr "もし関数が引数なしで呼ばれれば、デフォルト値は使われます"

