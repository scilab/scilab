<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>fr</LANGUAGE>
  <TITLE>ceil  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="ceil"> partie entière supérieure  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>[y]=ceil(x)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>x  </PARAM_NAME>
  <PARAM_DESCRIPTION>
  <SP>
    : matrice réelle
</SP>
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>y  </PARAM_NAME>
  <PARAM_DESCRIPTION>
  <SP>
    : matrice entière
</SP>
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    Renvoie dans y, pour chaque élément de x, le plus petit entier supérieur.
  </P>
  </DESCRIPTION>
  <EXAMPLE><![CDATA[
ceil([1.9 -2.5])-[2,-2]
ceil(-%inf)
x=rand()*10^20;ceil(x)-x
 ]]></EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>round</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>floor</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>int</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>
