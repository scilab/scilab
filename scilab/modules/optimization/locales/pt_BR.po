msgid ""
"\n"
"    primal obj.  dual obj.  dual. gap \n"
msgstr ""
"\n"
"    obj. primal  obj. dual fenda de dualidade \n"

#, c-format
msgid "%s: Accuracy insufficient to satisfy convergence criterion.\n"
msgstr ""
"%s: Precisão insuficiente para satisfazer o critério de convergência.\n"

#, c-format
msgid "%s: Argument 3: wrong number of columns %d expected\n"
msgstr "%s: Argumento 3: número incorreto de colunas, esperava-se %d\n"

#, c-format
msgid "%s: Cannot allocate more memory.\n"
msgstr "%s: Não é possível alocar mais memória.\n"

#, c-format
msgid "%s: Length of working array is too short.\n"
msgstr "%s: O comprimento do array é muito curto.\n"

#, c-format
msgid "%s: Q is not symmetric positive definite.\n"
msgstr "%s: Q não é simétrica positiva definida.\n"

#, c-format
msgid "%s: The constraints are inconsistent.\n"
msgstr "%s: As restrições são inconsistentes.\n"

#, c-format
msgid "%s: The minimization problem has no solution.\n"
msgstr "%s: O problema de minimização não tem solução.\n"

#, c-format
msgid "%s: Too many iterations (more than %d).\n"
msgstr "%s: Muitas iterações (mais de %d).\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: number of columns %d expected.\n"
msgstr ""
"%s: Tamanho incorreto para o argumento de entrada #%d: Esperava-se %d "
"colunas.\n"

#, c-format
msgid "Error in dgels, info = %d.\n"
msgstr "Erro em dgels, info = %d.\n"

#, c-format
msgid "Error in dspev, info = %d.\n"
msgstr "Erro em dspev, info = %d.\n"

#, c-format
msgid "Error in dspgv, info = %d.\n"
msgstr "Erro em dspgv, info = %d.\n"

#, c-format
msgid "Error in dspst, info = %d.\n"
msgstr "Erro em dspst, info = %d.\n"

#, c-format
msgid "Error in dtrcon, info = %d.\n"
msgstr "Erro em dtrcon, info = %d.\n"

msgid "F(x)*Z has a negative eigenvalue.\n"
msgstr "F(x)*Z tem um auto-valor negativo.\n"

msgid "L must be at least one.\n"
msgstr "L deve ser pelo menos um.\n"

msgid "Matrices Fi, i=1,...,m are linearly dependent.\n"
msgstr "As matrizes Fi, i=1,...,m são linearmente dependentes.\n"

msgid ""
"The matrices F_i, i=1,...,m are linearly dependent (or the initial points "
"are very badly conditioned).\n"
msgstr ""
"As matrizes F_i, i=1,...,m são linearmente dependentes (ou os pontos "
"iniciais são muito mal-condicionados).\n"

#, c-format
msgid "Work space is too small.  Need at least %d*sizeof(double).\n"
msgstr ""
"Espaço de trabalho muito pequeno.  É necessário pelo menos %d*sizeof"
"(double).\n"

msgid "Z0 does not satisfy equality conditions for dual feasibility.\n"
msgstr "Z0 não satisfaz às condições de igualdade para viabilidade dual.\n"

msgid "Z0 is not positive definite.\n"
msgstr "Z0 não é positivo definido.\n"

#, c-format
msgid "blck_szs[%d] must be at least one.\n"
msgstr "blck_szs[%d] deve ser pelo menos um.\n"

msgid "m must be at least one.\n"
msgstr "m deve ser pelo menos um.\n"

msgid "nu must be at least 1.0.\n"
msgstr "nu deve ser pelo menos 1.0.\n"

msgid "x0 is not strictly primal feasible.\n"
msgstr "x0 não é estritamente primal viável.\n"
