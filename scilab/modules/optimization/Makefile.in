install-libscioptimization_la_etcDATA: $(libscioptimization_la_etc_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(libscioptimization_la_etcdir)" || $(MKDIR_P) "$(DESTDIR)$(libscioptimization_la_etcdir)"
	@list='$(libscioptimization_la_etc_DATA)'; test -n "$(libscioptimization_la_etcdir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(libscioptimization_la_etcdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(libscioptimization_la_etcdir)" || exit $$?; \
	done

uninstall-libscioptimization_la_etcDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(libscioptimization_la_etc_DATA)'; test -n "$(libscioptimization_la_etcdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(libscioptimization_la_etcdir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(libscioptimization_la_etcdir)" && rm -f $$files
