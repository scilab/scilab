msgid "%s: Wrong type for input argument #%d: Real values expected.\n"
msgstr ""
"%s: Неверный тип входного аргумента #%d: Ожидались вещественные значения.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Matrix of floating point numbers "
"expected.\n"
msgstr ""
"%s: Неверный тип входного аргумента #%d: Ожидалась матрица чисел с плавающей "
"точкой.\n"

#, c-format
msgid "%s: Cannot open file '%s'.\n"
msgstr "%s: Не удалось открыть файл '%s'.\n"

