#, c-format
msgid "%s: Wrong type for input argument #%d: A string expected.\n"
msgstr "%s: Неверный тип входного аргумента #%d: Ожидалась строка.\n"

#, c-format
msgid "%s: Cannot go to directory %s\n"
msgstr "%s: Не удалось перейти в директорию %s\n"

