msgstr "   L Ma M Je V Sam Dim"

#, c-format
msgid "%s: An error occurred.\n"
msgstr "%s: Une erreur est survenue.\n"

#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s : Une erreur s'est produite : %s\n"

#, c-format
msgid "%s: Wrong number of input argument.\n"
msgstr "%s: Mauvais nombre d'argument d'entrée.\n"

#, c-format
msgid "%s: Wrong number of input argument: %d expected.\n"
msgstr "%s : Mauvais nombre d'arguments d'entrée : %d attendus.\n"

#, c-format
msgid "%s: Wrong number of input arguments.\n"
msgstr "%s : Mauvais nombre d'arguments d'entrée.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d or %d expected.\n"
msgstr "%s : Mauvais nombre d'arguments d'entrée : %d ou %d attendus.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Must be between %d and %d.\n"
msgstr ""
"%s: Mauvaise dimension pour l'argument d'entrée #%d: Cela doit être entre %d "
"et %d.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: m*3 matrix or a m*6 matrix expected.\n"
msgstr ""
"%s: Mauvaise dimension pour l'argument d'entrée n° %d: une matrice m*3 ou "
"une matrice m*6 attendue.\n"

#, c-format
msgid "%s: Wrong size for input argument: Same size expected.\n"
msgstr ""
"%s: Mauvaise dimension pour l'argument d'entrée : Même dimension attendue.\n"

#, c-format
msgid "%s: Wrong size for input arguments: Same size expected.\n"
msgstr ""
"%s : Mauvaise taille pour les arguments d'entrée : Mêmes tailles attendues.\n"

#, c-format
msgid ""
"%s: Wrong type  for input argument #%d: Real constant matrix expected.\n"
msgstr ""
"%s: Mauvais type pour l'argument d'entrée n° %d: Une matrice de constantes "
"réelles attendue.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer or '%s' expected.\n"
msgstr ""
"%s: Mauvais type pour l'argument d'entrée n° %d: Entier ou '%s' attendu.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr ""
"%s : Mauvais type pour l'argument d'entrée n°%d : Une matrice réelle "
"attendue.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real vector expected.\n"
msgstr ""
"%s: Mauvais type pour l'argument d'entrée n° %d: Un vecteur réel attendu.\n"

#, c-format
msgid "%s: Wrong type for input arguments.\n"
msgstr "%s: Mauvais type des arguments d'entrée.\n"

#, c-format
msgid "%s: Wrong type for input arguments: Integer expected.\n"
msgstr "%s: Mauvais type pour les arguments d'entrée: entiers attendus.\n"

#, c-format
msgid "%s: Wrong type for input arguments: Must be scalars.\n"
msgstr ""
"%s: Mauvais type pour les arguments d'entrée : ils doivent être des "
"scalaires.\n"

#, c-format
msgid "%s: Wrong type for input arguments: Scalar values expected.\n"
msgstr ""
"%s: Mauvais type pour les arguments d'entrée: Valeurs scalaires attendues.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: %s must be between %d and %d.\n"
msgstr ""
"%s: Mauvaise valeur pour l'argument d'entrée n° %d: %s doit être entre %d et "
"%d.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: ''%s'' or ''%s'' expected.\n"
msgstr ""
"%s : Mauvaise valeur pour l'argument d'entrée n°%d : ''%s'' ou ''%s'' "
"attendu.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Must be < %d.\n"
msgstr ""
"%s: Mauvaise valeur pour l'argument d'entrée n° %d: Doit être < %d.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Must be > %d.\n"
msgstr ""
"%s : Mauvaise valeur pour l'argument d'entrée n°%d : Doit être > %d.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Must be between %d and %d.\n"
msgstr ""
"%s : Mauvaise valeur pour l'argument d'entrée n°%d : Doit être entre %d et "
"%d.\n"

#, c-format
msgid "%s: Wrong value for input argument: Must be between %d and %d.\n"
msgstr ""
"%s: Mauvaise valeur pour l'argument d'entrée: Il doit être compris entre %d "
"et %d.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Non-negative integers expected.\n"
msgstr ""
"%s : Mauvaise valeurs pour l'argument d'entrée n°%d : Un entier non-négatif "
"attendu.\n"

