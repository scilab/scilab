#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr "%s: Неверное количество аргументов: Ожидалось от %d до %d.\n"

#, c-format
msgid "%s: Cannot open file %s.\n"
msgstr "%s: Не удалось открыть файл %s.\n"

#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: Произошла ошибка: %s\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Must be %s, an integer or a vector "
"of 2 integers.\n"
msgstr ""
"%s: Неверное значение аргумента #%d: Должно быть %s, целым числом или "
"вектором из двух целых.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument: Index range must be specified as a "
"scalar or 2-element vector.\n"
