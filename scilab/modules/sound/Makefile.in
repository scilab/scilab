swig-build: $(SWIG_FILES)
	@SWIG_PACKAGENAME=org.scilab.modules.$(modulename); \
	SWIG_OUTDIR=src/java/org/scilab/modules/$(modulename)/; \
	if test ! -z "$(SWIG_FILES)"; then \
