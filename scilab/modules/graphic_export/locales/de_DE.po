msgid "%s: Wrong size for input argument #%d: A scalar expected.\n"
msgstr "%s: Falsche Größe für Eingangsargument #%d: Skalar erwartet.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: Single character string expected.\n"
msgstr ""
"%s: Falsche Größe für Eingangsargument #%d: Einzelne Zeichenfolge erwartet.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A real between 0 and 1 expected.\n"
msgstr ""
"%s: Falscher Typ für Eingangsargument #%d: Kommazahl zwischen 0 und 1 "
"erwartet.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: An integer or a handle expected.\n"
msgstr ""

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr "%s: falscher Typ für Eingangsargument #%d: Integer erwartet.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Single character string expected.\n"
msgstr ""
"%s: falscher Typ für Eingangsargument #%d: ein einzelnes Zeichen erwartet.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr "%s: falscher Typ für Eingangsargument #%d: Variable erwartet.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: '%s' or '%s' expected.\n"
msgstr ""
"%s: falscher Wert für Eingangsargument #%d: '%s' oder '%s' erwartet.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: ''%s'' or ''%s'' expected.\n"
msgstr ""
"%s: falscher Wert für Eingangsargument #%d: ''%s'' oder ''%s'' erwartet.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: ''%s'', ''%s'', ''%s'', ''%s'', "
"''%s'' or ''%s'' expected.\n"
msgstr ""
"%s: falscher Wert für Eingangsargument #%d: ''%s'', ''%s'', ''%s'', ''%s'', "
"''%s'' oder ''%s'' erwartet.\n"

msgid "Please install Ghostscript 32 bits to export an .FIG file."
msgstr ""
"Bitte installieren Sie Ghostscript 32 Bits um eine .FIG Datei zu exportieren."

