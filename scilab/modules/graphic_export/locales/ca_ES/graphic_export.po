msgid "Scilab graphic module disabled -nogui or -nwni mode.\n"
msgstr "S'ha deshabilitat el mòdul gràfic Scilab en mode -nogui o -nwni.\n"

msgid "Scilab Graphic export module not installed.\n"
msgstr "El mòdul Graphic export de Scilab no està instal·lat.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: '%s' or '%s' expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d té un valor incorrecte. Ha de ser '%s' o '%s'.\n"

#, c-format
msgid "%s: OpenGL error during export.\n"
msgstr "%s: Hi ha hagut un error de OpenGL durant l'exportació.\n"

#, c-format
msgid "%s: Unable to create export file, permission denied.\n"
msgstr "%s: No es pot crear el fitxer d'exportació, permís denegat.\n"

#, c-format
msgid "%s: GL2PS error during export.\n"
msgstr "%s: Hi ha hagut un error de GL2PS durant l'exportació.\n"

#, c-format
msgid "%s: Unable to create export file, figure is too complex.\n"
msgstr ""
"%s: No es pot crear el fitxer d'exportació, la figura és massa complexa.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Positive integer expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d és d'un tipus incorrecte: Ha de ser un nombre "
"enter positiu.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Single character string expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d és d'un tipus incorrecte: Ha de ser un "
"caràcter.\n"

#, c-format
msgid "%s has been replaced by %s"
msgstr "S'ha reemplaçat %s per %s"

#, c-format
msgid "%s: %s must be called before any action.\n"
msgstr "%s: S'ha de cridar a %s abans de cada acció.\n"

#, c-format
msgid ""
"%s: Wrong number of input arguments: Function has no input argument with "
"''%s'' or ''%s'' drivers.\n"
msgstr ""
"%s: El nombre d'arguments d'entrada és incorrecte: La funció no té cap "
"argument d'entrada amb els controladors ''%s'' o ''%s''.\n"

#, c-format
msgid ""
"%s: Wrong number of input argument: String expected with ''%s'', ''%s'', "
"''%s'' or ''%s'' drivers.\n"
msgstr ""
"%s: El nombre d'arguments d'entrada és incorrecte: Ha de ser una cadena de "
"text amb els controladors ''%s'', ''%s'', ''%s'' o ''%s''.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d és d'un tipus incorrecte: Ha de ser un text.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr ""
"%s: Nombre incorrecte d'arguments d'entrada: Hi ha d'haver entre %d i %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr "%s: L'argument #%d és d'un tipus incorrecte: Ha de ser un enter.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: ''%s'' or ''%s'' expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d té un valor incorrecte: Ha de ser ''%s'' or "
"''%s''.\n"

#, c-format
msgid "%s: EMF format only available under Microsoft Windows OS.\n"
msgstr "%s: El format EMF només està disponible en Microsoft Windows.\n"

#, c-format
msgid "%s: Unable to execute pstoedit.\n"
msgstr "%s: No es pot executar el pstoedit.\n"

msgid ""
"xbasimp has been replaced by toprint for printing and by xs2ps for the file "
"export"
msgstr ""
"S'ha reemplaçat el xbasimp pel toprint per a imprimir i pel xs2ps per a "
"l'exportació del fitxer"

#, c-format
msgid ""
"%s: Unable to locate pstoedit. Please install pstoedit or add it to the PATH "
"to be able to use this feature.\n"
msgstr ""
"%s: No s'ha pogut trobar el pstoedit. Instal·leu el pstoedit o afegiu-lo al "
"PATH per poder utilitzar aquesta característica.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: ''%s'', ''%s'', ''%s'', ''%s'', "
"''%s'' or ''%s'' expected.\n"
msgstr ""
"%s: L'argument d'entrada #%d té un valor incorrecte: Ha de ser ''%s'', "
"''%s'', ''%s'', ''%s'', ''%s'' o ''%s''.\n"
