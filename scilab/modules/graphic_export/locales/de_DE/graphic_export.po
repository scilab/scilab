msgid "Scilab graphic module disabled -nogui or -nwni mode.\n"
msgstr "Grafisches Modul von Scilab deaktiviert: -nogui oder -nwni Modus.\n"

msgid "Scilab Graphic export module not installed.\n"
msgstr "Grafischer Exportmodus von Scilab nicht installiert.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: '%s' or '%s' expected.\n"
msgstr ""
"%s: falscher Wert für Eingangsargument #%d: '%s' oder '%s' erwartet.\n"

#, c-format
msgid "%s: OpenGL error during export.\n"
msgstr "%s: Fehler von OpenGL während des Exportierens.\n"

#, c-format
msgid "%s: Unable to create export file, permission denied.\n"
msgstr "%s: kann keine Datei zum Exportieren erzeugen, Vorgang abgelehnt.\n"

#, c-format
msgid "%s: GL2PS error during export.\n"
msgstr "%s: GL2PS Fehler während des Exportierens.\n"

#, c-format
msgid "%s: Unable to create export file, figure is too complex.\n"
msgstr "%s: kann keine Datei zum Exportieren erzeugen, Form zu komplex.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Positive integer expected.\n"
msgstr ""
"%s: falscher Typ für Eingangsargument #%d: positive Integerzahl erwartet.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Single character string expected.\n"
msgstr ""
"%s: falscher Typ für Eingangsargument #%d: ein einzelnes Zeichen erwartet.\n"

#, c-format
msgid "%s has been replaced by %s"
msgstr "%s wurde durch %s ersetzt"

#, c-format
msgid "%s: %s must be called before any action.\n"
msgstr "%s: %s muß vor jedem Vorgang aufgerufen werden.\n"

#, c-format
msgid ""
"%s: Wrong number of input arguments: Function has no input argument with "
"''%s'' or ''%s'' drivers.\n"
msgstr ""
"%s: falsche Anzahl an Eingangsargumenten: Funktion hat keine "
"Eingangsargumente mit '%s' oder '%s' Treibern.\n"

#, c-format
msgid ""
"%s: Wrong number of input argument: String expected with ''%s'', ''%s'', "
"''%s'' or ''%s'' drivers.\n"
msgstr ""
"%s: falsche Anzahl an Eingangsargumenten: Zeichenkette mit '%s', '%s', '%s' "
"oder '%s' Treibern erwartet.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr "%s: falscher Typ für Eingangsargument #%d: Variable erwartet.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr "%s: Falsche Anzahl an Eingangsargumenten: %d zu %d erwartet.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr "%s: falscher Typ für Eingangsargument #%d: Integer erwartet.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: ''%s'' or ''%s'' expected.\n"
msgstr ""
"%s: falscher Wert für Eingangsargument #%d: '%s' oder '%s' erwartet.\n"

#, c-format
msgid "%s: EMF format only available under Microsoft Windows OS.\n"
msgstr "%s: EMF Format nur unter MS Windows Betriebssystemen verfügbar.\n"

#, c-format
msgid "%s: Unable to execute pstoedit.\n"
msgstr "%s: kann pstoedit nicht ausführen.\n"

msgid ""
"xbasimp has been replaced by toprint for printing and by xs2ps for the file "
"export"
msgstr ""
"Xbasimp wurde für das Drucken durch toprint und durch xs2ps für den "
"Dateiexport ersetzt."

#, c-format
msgid ""
"%s: Unable to locate pstoedit. Please install pstoedit or add it to the PATH "
"to be able to use this feature.\n"
msgstr ""
"%s: kann pstoedit nicht lokalisieren. Installieren Sie pstoedit oder fügen "
"Sie dies zu PFAD hinzu, um diese Funktionalität zu verwenden.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: ''%s'', ''%s'', ''%s'', ''%s'', "
"''%s'' or ''%s'' expected.\n"
msgstr ""
"%s: falscher Wert für Eingangsargument #%d: '%s', '%s', '%s', '%s', '%s' "
"oder ''%s'' erwartet.\n"
