msgstr ""
"%s: Неверное значение входного параметра №%d: ожидалось «%s» или «%s».\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: 's' or 'r' expected.\n"
msgstr ""
"%s: Неверное значение входного параметра №%d: ожидалось «s» или «r».\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: 'u' (Upper) or 'l' (Lower) "
