			<real xcos="rpar">
				<map simName="Value" index="0,0"/>
			</real>
		</block>
		
<!-- Sinks -->
		<!-- Scop Block compatibility pattern -->
		<!-- FIXME -->
		<block sim="Scope" xcos="CSCOPE" description="">
			<simple sim="FunctionName" xcos="cscope"></simple>
			<simple sim="FunctionType" xcos="4"></simple>
			<simple sim="BlockType" xcos="c"></simple>
			<simple sim="ControlPort" xcos="1"></simple>
			<simple sim="CommandPort" xcos="0"></simple>
			<real xcos="exprs">
				<map simName="1 3 5 7 9 11 13 15" index="0,0"/> <!-- Color, mark, vector 8 entries -->
				<map simName="-1" index="1,0"/> <!-- Output window number (-1) default -->
				<map simName="[]" index="2,0"/> <!-- Output Window position -->
				<map simName="[600;400]" index="3,0"/> <!-- Output window size -->
				<map simName="YMin" index="4,0"/>
				<map simName="YMax" index="5,0"/>
				<map simName="30" index="6,0"/> <!-- RefreshPeriod -->
				<map simName="20" index="7,0"/> <!-- BufferSize -->
				<map simName="0" index="8,0"/> <!-- Accept herited events -->
				<map simName="Name of scope" index="9,0"/> <!-- Name of scope -->
			</real>	
			<real xcos="rpar">
				<map simName="0" index="0,0"/><!-- AcceptedHeritedEvents -->
				<map simName="YMin" index="1,0"/>
				<map simName="YMax" index="2,0"/>
				<map simName="30" index="3,0"/> <!-- RefreshPeriod -->
			</real>
			<real xcos="ipar">
				<map simName="-1" index="0,0"/> <!--  -->
				<map simName="1" index="1,0"/> <!--  -->
				<map simName="20" index="2,0"/> <!-- BufferSize -->
				<map simName="1" index="3,0"/> <!-- Color -->
				<map simName="3" index="4,0"/>
				<map simName="5" index="5,0"/>
				<map simName="7" index="6,0"/>
				<map simName="9" index="7,0"/>
				<map simName="11" index="8,0"/>
				<map simName="13" index="9,0"/>
				<map simName="15" index="10,0"/> <!-- mark -->
				<map simName="-1" index="11,0"/>
				<map simName="-1" index="12,0"/>
				<map simName="600" index="13,0"/> <!-- Window width -->
				<map simName="400" index="14,0"/> <!-- Height -->
			</real>
		</block>

<!-- Math Library Block Parameters -->
