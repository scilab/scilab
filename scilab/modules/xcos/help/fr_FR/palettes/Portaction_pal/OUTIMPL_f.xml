function [btn] = messagebox(msg, msgboxtitle, msgboxicon, buttons, ismodal)
	btn=1;
endfunction
warning("on")

importXcosDiagram(SCI + "/modules/xcos/examples/portaction_pal/fr_FR/OUTIMPL_f_fr_FR.zcos");
xcos_simulate(scs_m, 4);
]]></scilab:image>
