    <refsection id="Example_MATCATH">
        <title>Exemple</title>
        <programlisting role="example">
            A=[1 2 3;4 5 6]
            B=[7 8;9 10]
            Le résultat de la concaténation horizontale est:
            C=[1 2 3 7 8;4 5 6 9 10]
        </programlisting>
    </refsection>
