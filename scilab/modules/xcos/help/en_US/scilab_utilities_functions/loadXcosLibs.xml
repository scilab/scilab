    <refnamediv>
        <refname>loadXcosLibs</refname>
        <refpurpose>Load most of the Xcos macros</refpurpose>
    </refnamediv>
    <refsynopsisdiv>
        <title>Syntax</title>
        <synopsis>loadXcosLibs()</synopsis>
    </refsynopsisdiv>
    <refsection id="Module_loadXcosLibs">
        <title>Module</title>
        <itemizedlist>
            <listitem>
                <para>
                    <link linkend="xcos">xcos</link>
                </para>
            </listitem>
        </itemizedlist>
    </refsection>
    <refsection id="Description_loadXcosLibs">
        <title>Description</title>
        <para>Import some useful Xcos macros into Scilab.</para>
    </refsection>
    <refsection>
        <title>Examples</title>

        <programlisting role="example"><![CDATA[
exists("SUM_f")
loadXcosLibs();
exists("SUM_f")
typeof(SUM_f)
]]></programlisting>
    </refsection>
