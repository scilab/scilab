//      Multiple 1D transforms
A=[0   1   1
   2  -1  -1
   3   4   4
   5   1  -1];
y_ref = [ 10, 5, 3;
          -3+%i*3,-3+%i*2,-3;
          -4, 5, 7;
          -3-%i*3,-3-%i*2,-3];
y=fft(A,-1,1);
assert_checkalmostequal(y,y_ref);
y=fft(y_ref,1,1);
assert_checktrue(isreal(y));
assert_checkalmostequal(A,y);
y_ref = [ 2.5, 1.25, 0.75;
          -0.75-%i*0.75,-0.75-%i*0.5,-0.75;
          -1, 1.25, 1.75;
          -0.75+%i*0.75,-0.75+%i*0.5,-0.75];
y=fft(A,1,1);
assert_checkalmostequal(y,y_ref);
y=fft(y_ref,-1,1);
