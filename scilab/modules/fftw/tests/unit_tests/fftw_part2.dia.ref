y=fft(A);
assert_checktrue(isreal(y));
assert_checkalmostequal(y,y_ref);
y=fft(A,"symmetric");
assert_checktrue(isreal(y));
assert_checkalmostequal(y,y_ref);
y=fft(A,"nonsymmetric");
assert_checktrue(~isreal(y));
assert_checkalmostequal(y,y_ref);
