#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: сталася помилка: %s\n"

#, c-format
msgid "%s: Incompatible input arguments '#%d and '#%d': Same sizes expected.\n"
msgstr ""
"%s: несумісні вхідні параметри №%d і №%d: слід використовувати однакову "
"розмірність.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s: доступну пам’ять вичерпано.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d not expected.\n"
msgstr ""
"%s: помилкова кількість вхідних параметрів: не слід використовувати %d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d.\n"
msgstr "%s: помилкова розмірність вхідного параметра №%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: помилковий тип вхідного параметра №%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A Real or Complex expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати дійсне або "
"комплексне значення.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A string expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати тип "
"«string».\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: N-dimensional array expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати N-вимірний "
"масив.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: int matrix expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати матрицю з "
"цілих чисел.\n"

#, c-format
msgid "%s: Wrong values for input argument #%d.\n"
msgstr "%s: помилкові значення вхідного параметра №%d.\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: FFTW flag expected.\n"
msgstr ""
"%s: помилкові значення вхідного параметра №%d: слід використовувати "
"прапорець FFTW.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Non-negative integers expected.\n"
msgstr ""
"%s: помилкові значення вхідного параметра №%d: слід використовувати "
"невід’ємні цілі числа\n"

#, c-format
msgid "FFTW Library %s not found.\n"
msgstr "Бібліотеки FFTW %s не виявлено.\n"

msgid "FFTW Library not found.\n"
msgstr "Бібліотеки FFTW не виявлено.\n"

msgid "FFTW can't read wisdom."
msgstr "FFTW не може прочитати файл оптимізації (див. fftw-wisdom)."

msgid "Scilab FFTW module not installed.\n"
msgstr "Модуль FFTW Scilab не встановлено.\n"
