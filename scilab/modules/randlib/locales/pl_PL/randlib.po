#, c-format
msgid "%s: Wrong number of output argument: %d expected with option '%s'.\n"
msgstr ""
"%s: Nieprawidłowa liczba argumentów wyjściowych: %d oczekiwana opcja z "
"'%s'.\n"

#, c-format
msgid ""
"%s: Wrong second input argument: Square matrix or row vector expected.\n"
msgstr ""
"%s: Nieprawidłowy drugi argument wejściowy : oczekiwana macierz kwadratowa "
"lub wektor wierszowy.\n"

#, c-format
msgid "%s: Wrong size for input argument: Column vector expected.\n"
msgstr ""
"%s: Nieprawidłowy rozmiar argumentu wejściowego: oczekiwany wektor "
"kolumnowy.\n"

#, c-format
msgid "%s: Wrong size for input argument: Scalar expected for %s.\n"
msgstr ""
"%s: Nieprawidłowy rozmiar argumentu wejściowego: oczekiwany skalar dla %s.\n"

#, c-format
msgid "%s: Wrong size for input arguments: Mean and Cov are of null size.\n"
msgstr ""
"%s: Nieprawidłowy rozmiar argumentu wejściowego: średnie i Cov mają rozmiar "
"null.\n"

#, c-format
msgid "%s: Wrong size for second input argument: %dx%d expected.\n"
msgstr ""
"%s: Nieprawidłowy  rozmiar drugiego argumentu wejściowego: %dx%d "
"oczekiwany.\n"

#, c-format
msgid "%s: Wrong type for fifth input argument: Scalar expected.\n"
msgstr ""
"%s: Nieprawidłowy piąty typ argumentu wejściowego: oczekiwany skalar.\n"

#, c-format
msgid "%s: Wrong type for first input argument: Scalar expected.\n"
msgstr ""
"%s: Nieprawidłowy pierwszy typ argumentu wejściowego: oczekiwany skalar.\n"

#, c-format
msgid "%s: Wrong type for fourth input argument: Scalar expected.\n"
msgstr ""
"%s: Nieprawidłowy czwarty typ argumentu wejściowego: oczekiwany skalar.\n"

#, c-format
msgid "%s: Wrong type for input argument: Column vector expected.\n"
msgstr ""
"%s: Nieprawidłowy typ argumentu wejściowego: oczekiwany wektor kolumnowy.\n"

#, c-format
msgid "%s: Wrong type for input argument: Scalar expected.\n"
msgstr "%s: Nieprawidłowy typ argumentu wejściowego: oczekiwany skalar.\n"

#, c-format
msgid "%s: Wrong type for input argument: Square matrix expected.\n"
msgstr ""
"%s: Nieprawidłowy typ argumentu wejściowego: oczekiwana macierz kwadratowa.\n"

#, c-format
msgid ""
"%s: Wrong type for input arguments: Mean and Cov have incompatible "
"dimensions\n"
msgstr ""
"%s: Nieprawidłowy typ argumentu wejściowego: Średnia i Cov mają niezgodny "
"wymiar\n"

#, c-format
msgid "%s: Wrong type for second input argument: Scalar expected.\n"
msgstr ""
"%s: Nieprawidłowy typ drugiego argumentu wejściowego: oczekiwany skalar.\n"

#, c-format
msgid "%s: Wrong type for third input argument: Scalar expected.\n"
msgstr ""
"%s: Nieprawidłowy typ trzeciego argumentu wejściowego: oczekiwany skalar.\n"

#, c-format
msgid ""
"%s: Wrong value for first input argument: Must be the number of random "
"deviate.\n"
msgstr ""
"%s: Nieprawidłowa wartość pierwszego argumentu wejściowego: musi być liczbą "
"losowo odbiegającą.\n"

#, c-format
msgid ""
"%s: Wrong value for first input argument: Must be the number of random "
"simulation.\n"
msgstr ""
"%s: Nieprawidłowa wartość pierwszego argumentu wejściowego: muszą być liczby "
"losowej symulacji\n"

#, c-format
msgid "%s: Wrong value for input argument %s.\n"
msgstr "%s: Nieprawidłowa wartość argumentu wejściowego %s.\n"

#, c-format
msgid "%s: Wrong value for input argument: Must be between '%s' and %d.\n"
msgstr ""
"%s: Nieprawidłowa wartość argumentu wejściowego: Musi być pomiędzy '%s' i "
"%d.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument: Number of random simulation expected.\n"
msgstr ""
"%s: Nieprawidłowa wartość argumentu wejściowego: oczekiwana losowa liczba "
"symulacji.\n"

#, c-format
msgid "%s: Wrong value for second input argument: %d, %d or %d expected.\n"
msgstr ""
"%s: Nieprawidłowa wartość drugiego argumentu wejściowego: %d,%d lub %d "
"oczekiwana.\n"

#, c-format
msgid ""
"%s: Wrong value for second input argument: '%s', '%s', '%s', '%s', '%s' or "
"'%s' expected.\n"
msgstr ""
"%s: Nieprawidłowa wartość drugiego argumentu wejściowego: '%s', '%s', '%s', "
"'%s', '%s' lub  '%s'  oczekiwana.\n"

#, c-format
msgid "%s: Wrong value for second input argument: Must be > %d.\n"
msgstr ""
"%s: Nieprawidłowa wartość dla drugiego argumentu wejściowego: musi być > "
"%d.\n"

#, c-format
msgid ""
"%s: Wrong value for second input argument: Must be between %d and %d.\n"
msgstr ""
"%s: Nieprawidłowa wartość drugiego argumentu wejściowego: Musi być między %d "
"i %d.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument: Vector of %d or %d values for %s "
"expected.\n"
msgstr ""
"%s: Nieprawidłowa wartość dla argumentu wejściowego: Oczekiwano wartości dla "
"wektora z %d lub %d %s .\n"

