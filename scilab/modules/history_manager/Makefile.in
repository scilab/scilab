install-libscihistory_manager_la_etcDATA: $(libscihistory_manager_la_etc_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(libscihistory_manager_la_etcdir)" || $(MKDIR_P) "$(DESTDIR)$(libscihistory_manager_la_etcdir)"
	@list='$(libscihistory_manager_la_etc_DATA)'; test -n "$(libscihistory_manager_la_etcdir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(libscihistory_manager_la_etcdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(libscihistory_manager_la_etcdir)" || exit $$?; \
	done

