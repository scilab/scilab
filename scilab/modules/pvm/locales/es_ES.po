#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: A ocurrido un error: %s\n"

#, c-format
msgid "%s: Error while creating processes: %s.\n"
msgstr "%s: Error mientras se creaban los procesos: %s.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s: No hay mas memoria.\n"

#, c-format
msgid "%s: No such file or directory.\n"
msgstr "%s: No hay tal archivo o directorio.\n"

#, c-format
msgid "%s: Unknown or not yet implemented type.\n"
msgstr "%s: Tipo desconocido o no implementado.\n"

#, c-format
msgid "%s: Wrong first input argument: '%s' or '%s' expected.\n"
msgstr ""
"%s: Primer argumento de entrada incorrecto: Se esperaba '%s' o '%s'.\n"

#, c-format
msgid "%s: workspace (stacksize) is too small.\n"
msgstr "%s: espacio de trabajo (stacksize) demasiado pequeño.\n"

msgid "(group,instance) already exists"
msgstr "(grupo, instancia) ya existe"

msgid "Already doing operation"
msgstr "Ya está haciendo la operación"

msgid "Already in group"
msgstr "Ya está en el grupo"

msgid "Bad message id"
msgstr "Identificación de mensaje incorrecta"

msgid "Bad parameter"
msgstr "Parámetro erróneo"

msgid "Barrier count mismatch"
msgstr "Inconsistencia en el contador de Barrera"

msgid "Can not decode received msg"
msgstr "No se puede decodificar el mensaje recibido"

msgid "Can not get memory"
msgstr "No se puede obtener memoria"

msgid "Daemon pvmd is not responding"
msgstr "El demonio pvmd no está respondiendo"

msgid "Error MALLOC in pvm_error\n"
msgstr "Error MALLOC (asignación de memoria) en pvm_error\n"

msgid "Error pvm_halt - signal\n"
msgstr "Error pvm_halt - signal\n"

msgid "Error pvm_reduce: Not scalar type\n"
msgstr "Error pvm_reduce: No es de tipo escalar\n"

#, c-format
msgid "Error pvm_reduce: Unknown reduction operation %s\n"
msgstr "Error pvm_reduce: operación de reducción %s desconocida\n"

msgid "Failed to exec new slave pvmd"
msgstr "Falló la ejecución del nuevo esclavo pvmd"

msgid "Function not implemented"
msgstr "Función no implementada"

msgid "Host already configured (Duplicate host)"
msgstr "Ya se configuró el host (Host duplicado)"

msgid "Host failed"
msgstr "Falló host"

msgid "No current buffer"
msgstr "No hay buffer actual"

msgid "No group with that name"
msgstr "No hay grupo con ese nombre"

msgid "No parent task"
msgstr "No hay tarea padre"

msgid "No such (group,instance)"
msgstr "No hay tal (grupo, instancia)"

msgid "No such executable"
msgstr "No hay tal ejecutable"

msgid "No such host"
msgstr "No hay tal host"

msgid "No such instance in group"
msgstr "No hay tal instancia en el grupo"

msgid "No such task"
msgstr "No hay tal tarea"

msgid "Not in group"
msgstr "No está en el grupo"

msgid "Null group name is illegal"
msgstr "El nombre del grupo Null es ilegal"

msgid "OK"
msgstr "OK"

msgid "Out of resources"
msgstr "Recursos agotados"

msgid "PVM exists"
msgstr "Existe PVM"

msgid "PVM not found"
msgstr "No se encontró PVM"

msgid "PVM parallel Toolbox"
msgstr "Barra de herramientas de la Máquina Virtual Paralela"

msgid "Pvmd system error"
msgstr "Error del sistema pvmd"

msgid "Pvmd-pvmd protocol mismatch"
msgstr "Inconsistencia de protocolo pvmd-pvmd"

msgid "Read past end of buffer"
msgstr "Leer último final del buffer"

msgid "Scilab PVM module not installed.\n"
msgstr "El módulo PVM de Scilab no está instalado.\n"

#, c-format
msgid "The configuration file %s is used.\n"
msgstr "Se usa el archivo de configuración %s.\n"

#, c-format
msgid ""
"The standard configuration file $SCI%s will be used.\n"
"With SCI=%s\n"
"SCI will have to be set on remote hosts\n"
"in order to spawn scilab\n"
msgstr ""
"Se usará el archivo de configuración estándar $SCI%s.\n"
"Con SCI=%s\n"
"SCI deberá ser configurado en los hosts remotos\n"
"para poder iniciar Scilab\n"

#, c-format
msgid ""
"The standard configuration file $SCI%s%s needs the environment variables %s, "
"%s, %s to be set. PVM can not be started.\n"
msgstr ""
"El archivo de configuración estándar $SCI%s%s necesita que las variables de "
"entorno %s, %s, %s sean configuradas. No se puede iniciar PVM.\n"

msgid "Unknown Error"
msgstr "Error desconocido"

msgid "Warning: PVM_ROOT is not set.\n"
msgstr "Advertencia: PVM_ROOT no fue asignado.\n"

#, c-format
msgid "Warning: PVM_ROOT is set to %s\n"
msgstr "Advertencia: PVM_ROOT es %s\n"

#, c-format
msgid ""
"Warning: The standard configuration file $SCI%s was not found.\n"
"We supposed that PVM and scilab are in standard place on your net\n"
" (Cf. man pvmd3)\n"
msgstr ""
"Advertencia: El archivo de configuración estándar $SCI%s no fue encontrado.\n"
"Suponemos que PVM y scilab están en un lugar estándar de su red\n"
" (Cf. man pvmd3)\n"

msgid "but there exists no configuration file:\n"
msgstr "pero no existe archivo de configuración:\n"

