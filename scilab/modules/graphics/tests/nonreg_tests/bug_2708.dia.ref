// <-- Short Description -->
// It is no more possible to fill the user_data field of a graphic object with lists
// Pierre MARECHAL - Scilab Project
// Copyright INRIA
// 10/02/2008
a=gca();
set(a,'user_data',tlist(['gindex','node','edge'],[],[]));
if or( a.user_data <> tlist(['gindex','node','edge'],[],[]) ) then bugmes();quit;end
set(a,'user_data',list());
if or( a.user_data <> list() ) then bugmes();quit;end
