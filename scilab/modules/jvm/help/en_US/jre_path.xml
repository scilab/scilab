<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>jre_path</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>$LastChangedDate: 2006-12-12 09:26:08 +0200$</DATE>

  <SHORT_DESCRIPTION name="jre_path">returns Java Runtime Environment used by Scilab</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>p=jre_path()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>p</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: a string path of JRE</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>returns Java Runtime Environment used by Scilab.</P>
  </DESCRIPTION>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>system_getproperty</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>

  <AUTHOR>A.C</AUTHOR>
</MAN>