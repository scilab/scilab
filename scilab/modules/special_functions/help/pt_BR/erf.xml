                <link linkend="erfc">erfc</link>
            </member>
            <member>
                <link linkend="erfcx">erfcx</link>
            </member>
            <member>
                <link linkend="erfinv">erfinv</link>
            </member>
        </simplelist>
    </refsection>
</refentry>
