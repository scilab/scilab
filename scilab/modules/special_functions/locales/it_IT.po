#, c-format
msgid "%s: Only one of arg1 and arg2 may be a vector.\n"
msgstr "%s: Solo uno tra arg1 e arg2 può essere un vettore.\n"

#, c-format
msgid "%s: Wrong type for first input argument.\n"
msgstr "%s: Il primo argomento in ingresso è di tipo sbagliato.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d or #%d: No complex input argument "
"expected.\n"
msgstr ""
"%s: L'argomento #%d o #%d è di tipo sbagliato: non erano attesi parametri in "
"ingresso di tipo complesso.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: L'argomento in ingresso #%d è di tipo sbagliato.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d è di tipo sbagliato: era atteso un numero "
"reale.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d è di tipo sbagliato: era attesa una matrice "
"reale.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Matrix with elements in (%d,%d) "
"expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d ha un valore sbagliato: era attesa una "
"matrice con elementi in (%d,%d).\n"

#, c-format
msgid "%s: Wrong value for input arguments: Must be > %d.\n"
msgstr ""
"%s: Il valore degli argomenti in ingresso è sbagliato: deve essere >%d.\n"

#, c-format
msgid "%s: error number %d\n"
msgstr "%s: errore numero %d\n"

#, c-format
msgid "%s: overflow or underflow of an extended range number\n"
msgstr "%s: overflow o underflow di un numero di portata estesa.\n"

#, c-format
msgid "Function %s is obsolete."
msgstr "La funzione %s è obsoleta."

#, c-format
msgid "Please use %s instead."
msgstr "Usare %s invece."

#, c-format
msgid "This function will be permanently removed in Scilab %s"
msgstr "Questa funzione sarà rimossa in modo permanente in Scilab %s"

msgid "Warning: "
msgstr "Attenzione: "
