#, c-format
msgid "%s: Wrong type for input argument #%d: Real expected.\n"
msgstr "%s: Mauvais type pour l'argument d'entrée n° %d: Réel attendu.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d or #%d: No complex input argument "
"expected.\n"
msgstr ""
"%s: Mauvais type pour l'argument d'entrée n° %d ou n° %d: Pas d'argument "
"d'entrée complexe attendu.\n"

#, c-format
msgid "%s: Wrong value for input arguments: Must be > %d.\n"
msgstr "%s: Mauvaise valeur pour les arguments d'entrée: Doit être > %d.\n"

#, c-format
msgid "%s: Wrong type for first input argument.\n"
msgstr "%s: Mauvais type pour le premier argument en entrée.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: Mauvais type pour l'argument d'entrée n° %d.\n"

#, c-format
msgid "%s: Only one of arg1 and arg2 may be a vector.\n"
msgstr "%s: Seulement un de arg1 et arg2 doit être un vecteur.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr ""
"%s: Mauvais type pour l'argument d'entrée n° %d: Une matrice réelle "
"attendue.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Matrix with elements in (%d,%d) "
"expected.\n"
msgstr ""
"%s: Mauvaise valeur pour l'argument d'entrée n° #%d: Matrice avec des "
"éléments dans (%d,%d) attendue.\n"

#, c-format
msgid "%s: overflow or underflow of an extended range number\n"
msgstr "%s: débordement d'un nombre à précision étendu\n"

#, c-format
msgid "%s: error number %d\n"
msgstr "%s: numéro d'erreur %d\n"
