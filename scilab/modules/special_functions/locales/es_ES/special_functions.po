#, c-format
msgid "%s: Wrong type for input argument #%d: Real expected.\n"
msgstr ""
"%s: Tipo incorrecto del argumento de entrada #%d: Se esperaba un real.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d or #%d: No complex input argument "
"expected.\n"
msgstr ""
"%s: Tipo incorrecto de los argumentos de entrada #%d o #%d: No se esperaba "
"un argumento complejo.\n"

#, c-format
msgid "%s: Wrong value for input arguments: Must be > %d.\n"
msgstr "%s: Valor incorrecto para el argumento de entrada: Debe ser > %d.\n"

#, c-format
msgid "%s: Wrong type for first input argument.\n"
msgstr "%s: Tipo incorrecto para el primer argumento de entrada.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: Tipo incorrecto para el argumento de entrada #%d.\n"

#, c-format
msgid "%s: Only one of arg1 and arg2 may be a vector.\n"
msgstr "%s: Solo arg1 ó arg2 puede ser un vector.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr ""
"%s:Tipo incorrecto del argumento de entrada #%d: Se esperaba una matriz "
"real.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Matrix with elements in (%d,%d) "
"expected.\n"
msgstr ""
"%s: Valor incorrecto del argumento de entrada #%d: Se esperaba una matriz "
"con elementos en (%d,%d).\n"

#, c-format
msgid "%s: overflow or underflow of an extended range number\n"
msgstr ""
"%s: sobredesbordamiento o subdesbordamiento (over/underflow) de un número de "
"rango extendido\n"

#, c-format
msgid "%s: error number %d\n"
msgstr "%s: Número de error %d\n"
