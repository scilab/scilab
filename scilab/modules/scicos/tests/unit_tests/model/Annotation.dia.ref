// Overload scicos_getvalue and xstringl to customize parameters
function [ok,txt,font,siz,exprs] = scicos_getvalue(title, fields, types, exprs)
    ok = %t;
    txt = "newText";
    font = 3;
    siz = 4;
    exprs = [txt; string(font); string(siz)];
endfunction
function r = xstringl(a, b, c, d, e)
    r = [0 0 3 4];
endfunction
