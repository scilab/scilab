#, c-format
msgid " %s.mo cannot be compiled\n"
msgstr " %s.mo no puede ser compilado\n"

#, c-format
msgid " %s.mo compilation forced\n"
msgstr " Se forzó la compilación de %s.mo\n"

#, c-format
msgid " %s.mo must be recompiled\n"
msgstr " %s.mo debe ser recompilado\n"

#, c-format
msgid "%s: An error occurred during the compilation of the Modelica block."
msgstr "%s: A ocurrido error mientras se compilaba el bloque Modelica."

#, c-format
msgid "%s: Cannot find any MO files in the following location: %s.\n"
msgstr ""
"%s: No se puede encontrar ningún archivo MO en la siguiente ubicación: %s.\n"

#, c-format
msgid "%s: Diagram does not compile in pass %d.\n"
msgstr "%s: El diagrama no compila en %d.\n"

#, c-format
msgid "%s: Error : the following command line failed to execute: %s.\n"
msgstr "%s: Error : la siguiente línea de orden falló en su ejecución: %s.\n"

#, c-format
msgid "%s: Error: A scifunc block has not been defined."
msgstr "%s: Error: No se definió un bloque scifunc."

#, c-format
msgid "%s: Error: Modelica compiler (MODELICAC) is unavailable."
msgstr "%s: Error: El compilador Modelica (MODELICAC) no está disponible."

#, c-format
msgid "%s: Input ports are not numbered properly.\n"
msgstr "%s: Los puertos de entrada no están numerados correctamente.\n"

#, c-format
msgid "%s: Output ports are not numbered properly.\n"
msgstr "%s: Los puertos de salida no están numerados correctamente.\n"

#, c-format
msgid "%s: Wrong number of input arguments. Must be between %d and %d.\n"
msgstr ""
"%s: Número incorrecto de argumentos de entrada. Debe estar entre %d y %d.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d expected.\n"
msgstr "%s: Número incorrecto de argumentos de entrada: Se esperaban %d.\n"

#, c-format
msgid "%s: Wrong number of output arguments: %d expected.\n"
msgstr "%s: Número incorrecto de argumentos de salida: Se esperaban %d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Single integer expected.\n"
msgstr ""
"%s: Tamaño incorrecto del argumento de entrada #%d: Se esperaba un entero "
"simple.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Single string expected.\n"
msgstr ""
"%s: Tamaño incorrecto del argumento de entrada #%d: Se esperaba un string "
"simple.\n"

#, c-format
msgid "%s: Wrong size for input arguments #%d and #%d.\n"
msgstr "%s: Tamaño incorrecto de los argumentos de entrada #%d y #%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr ""
"%s: Tipo incorrecto del argumento de entrada #%d: Se esperaba un entero.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr ""
"%s: Tipo incorrecto del argumento de entrada #%d: Se esperaba un string.\n"

msgid "&?"
msgstr "&?"

msgid "&Edit"
msgstr "&Editar"

msgid "&File"
msgstr "Archivo (&F)"

msgid "&Tools"
msgstr "Herramien&tas"

msgid "''scicos'' is no more available, please use ''xcos'' instead.\n"
msgstr ""
"''scicos'' ya no está disponible, por favor en su lugar utilice ''xcos''.\n"

msgid "A compatible C compiler required."
msgstr "Se requiere un compilador C compatible."

msgid "Only one block can be selected in current window for this operation."
msgstr ""
"Sólo se puede seleccionar un bloque en la ventana actual para esta operación."

#, c-format
msgid "Problem while building library %s\n"
msgstr "Problema creando la biblioteca %s\n"

msgid "Sorry compiling problem"
msgstr "Disculpe, ocurrió un error de compilación"

msgid "The current icon depends on block parameter\n"
msgstr "El icono actual depende de un parámetro de bloque\n"
