#, c-format
msgid "%s: Answer appears to be lower than lowest search bound %g\n"
msgstr "%s: відповідь лежить нижче за нижнє обмеження пошуку %g\n"

#, c-format
msgid "%s: Answer appears to be higher than greatest search bound %s\n"
msgstr "%s: відповідь лежить вище за верхнє обмеження пошуку %s\n"

#, c-format
msgid "%s: Answer appears to be higher than greatest search bound %f\n"
msgstr "%s: відповідь лежить вище за верхнє обмеження пошуку %f\n"

#, c-format
msgid "%s: Std must not be zero\n"
msgstr "%s: Std має бути ненульовим значенням\n"

#, c-format
msgid "%s: cumgam returned an error\n"
msgstr "%s: функцією cumgam повернуто повідомлення про помилку\n"

#, c-format
msgid "%s: gamma or inverse gamma routine failed\n"
msgstr ""
"%s: помилка під час роботи підпрограм обчислення гамма-функції або оберненої "
"гамма-функції\n"

#, c-format
msgid "%s: Argument #%d out of range. Bound exceeded: %f.\n"
msgstr ""
"%s: параметр №%d лежить поза межами припустимого діапазону. Перевищено "
"обмеження: %f.\n"

#, c-format
msgid "%s: Incompatible input arguments #%d and #%d': Same sizes expected.\n"
msgstr ""
"%s: несумісні вхідні параметри №%d і №%d: розмірності параметрів мали бути "
"однаковими.\n"

#, c-format
msgid ""
"%s: Warning: using non integer values for argument #%d may lead to incorrect "
"results.\n"
msgstr ""
"%s: попередження: використання нецілих значень для параметра №%d може "
"призвести до помилкових результатів.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A real expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати тип дійсних "
"значень.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: An integer expected.\n"
msgstr "%s: помилковий тип вхідного параметра №%d: мало бути ціле число.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A real expected.\n"
msgstr ""
"%s: помилковий розмір вхідного параметра №%d: слід використовувати дійсне "
"число.\n"

#, c-format
