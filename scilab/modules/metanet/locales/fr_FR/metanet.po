msgid "Metanet"
msgstr "Metanet"

msgid "&File"
msgstr "&Fichier"

msgid "&Tools"
msgstr "&Outils"

msgid "&Edit"
msgstr "&Edition"

msgid "&?"
msgstr "&?"

#, c-format
msgid "%s: First argument must be a vector of size greater or equal to 2.\n"
msgstr ""
"%s: Le premier argument doit être un vecteur de taille égale ou plus grande "
"que 2.\n"

#, c-format
msgid ""
"%s: Second argument must be a vector with same dimension as the first one.\n"
msgstr ""
"%s: Le second argument doit être un vecteur ayant les même dimension que le "
"premier.\n"

#, c-format
msgid "%s: Third argument must be a vector of size greater or equal to 1.\n"
msgstr ""
"%s: Le troisième argument doit être un vecteur de taille égale ou plus "
"grande que 1.\n"

#, c-format
msgid "%s: First argument must be a vector with positive integer values.\n"
msgstr ""
"%s: Le premier argument doit être un vecteur composé de valeurs entières "
"positives.\n"

#, c-format
msgid "%s: Second argument must be a vector with positive integer values.\n"
msgstr ""
"%s: Le second argument doit être un vecteur composé de valeurs entières "
"positives.\n"

#, c-format
msgid "%s: Third argument must be a vector with positive integer values.\n"
msgstr ""
"%s: Le troisième argument doit être un vecteur composé de valeurs entières "
"positives.\n"

msgid "Running out of memory"
msgstr "Plus de mémoire disponible"

#, c-format
msgid "\"%s\" is a directory"
msgstr "« %s » est un répertoire"

#, c-format
msgid "Directory \"%s\" does not exist"
msgstr "Le répertoire \"%s\" n'existe pas"

#, c-format
msgid "Graph file \"%s/%s.graph\" does not exist"
msgstr "Le fichier graphe \"%s/%s.graph\" n'existe pas"

#, c-format
msgid "Unable to open file \"%s/%s.graph\""
msgstr "Impossible d'ouvrir le fichier \"%s/%s.graph\""

#, c-format
msgid "Bad graph file. Node \"%s\" is duplicated"
msgstr "Mauvais fichier graphe. Le noeud \"%s\" est dupliqué"

#, c-format
msgid "Bad graph file. Node \"%s\" referenced by arc \"%s\" not found"
msgstr ""
"Mauvais fichier graphe. Le noeud \"%s\" référencé par l'arc \"%s\" est "
"introuvable"

msgid "Scilab module metanet not installed.\n"
msgstr "Le module Scilab metanet n'est pas installé.\n"

#, c-format
msgid "Bad internal node number %d"
msgstr "Mauvais numéro interne de noeud %d"

#, c-format
msgid "Bad internal arc number %d"
msgstr "Mauvais numéro interne d'arc %d"

#, c-format
msgid "Unable to write file in directory %s, check access"
msgstr ""
"Impossible d'écrire le fichier dans le répertoire %s, vérifiez les droits"
