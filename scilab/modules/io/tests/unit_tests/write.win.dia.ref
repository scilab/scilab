function check_file(computed_file, ref_file)
    fd = mopen(computed_file, "r");
    fdr = mopen(ref_file, "r");
    computed = mgetl(fd);
    expected = mgetl(fdr);
    assert_checkequal(expected, computed);
    mclose(fd);
    mclose(fdr);
endfunction
