#, c-format
msgid "%s: Can not read input argument #%d.\n"
msgstr "%s: Impossibile leggere l'argomento di input #%d.\n"

#, c-format
msgid "%s: Wrong type for argument %d: Real matrix expected.\n"
msgstr ""
"%s: L'argomento in ingresso %d è di tipo sbagliato: era attesa una matrice "
"reale.\n"

#, c-format
msgid "%s: Wrong type for argument %d: Real scalar expected.\n"
msgstr ""
"%s: L'argomento in ingresso %d è di tipo sbagliato: era atteso uno scalare "
"reale.\n"

#, c-format
msgid "%s: Wrong size for argument %d: Real scalar expected.\n"
msgstr ""
"%s: La grandezza dell'argomento in ingresso %d è di tipo sbagliato: era "
"atteso uno scalare reale.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s: Memoria esaurita.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: An integer value expected.\n"
msgstr ""
"%s: L'argomento in ingresso %d è di tipo sbagliato: era atteso un'intero.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Must be in the set {%s}.\n"
msgstr ""
"%s: L'argomento in ingresso #%d è di tipo sbagliato: deve essere "
"nell'insieme {%s}.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument: '%s', '%s', '%s', '%s' or '%s' "
"expected.\n"
msgstr ""
"%s: L'argomento in ingresso ha un valore sbagliato: era atteso '%s', '%s', "
"'%s', '%s' o '%s'.\n"
