#, c-format
msgid "%s: Can not read input argument #%d.\n"
msgstr "%s : Impossible de lire l'argument d'entrée n°%d.\n"

#, c-format
msgid "%s: Wrong type for argument #%d: Real scalar expected.\n"
msgstr "%s : Type erroné de l'argument %d : Un scalaire réel attendu.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s : Plus de mémoire disponible.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d expected.\n"
msgstr "%s : Nombre erroné d'arguments d'entrée : %d attendu(s).\n"

#, c-format
msgid ""
"%s: Wrong type of input arguments: integer, boolean or double expected.\n"
msgstr ""
"%s : Types erronés des arguments d'entrée : entiers, booléens ou réels "
"attendus.\n"

msgid "Never occur"
msgstr "Aucune occurrence"

#, c-format
msgid "%s: Wrong number of input argument(s): %d expected.\n"
msgstr "%s : Nombre erroné d'argument(s) d'entrée : %d attendu(s).\n"

#, c-format
msgid "%s: Wrong number of output argument(s): %d expected.\n"
msgstr "%s : Nombre erroné d'argument(s) de sortie : %d attendu(s).\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: %s, %s or %s expected.\n"
msgstr "%s : Type erroné de l'argument d'entrée #%d : %s, %s ou %s attendu.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d : int or double expected.\n"
msgstr ""
"%s : Type erroné de l'argument d'entrée #%d : entier ou réel attendu.\n"

#, c-format
msgid "%s: '%s' already defined by the '%s' argument."
msgstr "%s : '%s' est déjà défini par l'argument '%s'"

#, c-format
msgid ""
"%s: Wrong value for input argument: '%s', '%s', '%s', '%s' or '%s' "
"expected.\n"
msgstr ""
"%s : Valeur erronée de l'argument d'entrée : \"%s\", \"%s\", \"%s\", \"%s\" "
"ou \"%s\" attendu.\n"

#, c-format
msgid "%s: Non implemented feature.\n"
msgstr "%s : Fonction non implémentée\n"

#, c-format
msgid "%s: %s expected instead of %s.\n"
msgstr "%s : %s attendu au lieu de %s.\n"

#, c-format
msgid "%s: Wrong number of input argument(s): %d to %d expected.\n"
msgstr "%s : Nombre erroné d'argument(s) d'entrée : %d à %d attendu(s).\n"

#, c-format
msgid "%s: Wrong type for argument #%d: string expected.\n"
msgstr ""
"%s : Type erroné de l'argument n°%d : une chaîne de caractères attendue.\n"

#, c-format
msgid "%s: Wrong size for argument #%d: A single string expected.\n"
msgstr ""
"%s : Taille erronée de l'argument n°%d : une seule chaîne de caractères "
"attendue.\n"

#, c-format
msgid ""
"%s: Wrong size for argument #%d: A single string of size %d to %d expected.\n"
msgstr ""
"%s : Taille erroné de l'argument n°%d : une seule chaîne de caractères de "
"taille %d à %d attendue.\n"

#, c-format
msgid "%s: Incorrect integer type: %s .\n"
msgstr "%s : Type d'entier erroné : %s.\n"
