msgid "Signal Processing"
msgstr "Обработка сигналов"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Must be in the interval [%s, %s].\n"
msgstr ""
"%s: Неверное значение аргумента #%d: Должно быть в интервале [%s, %s].\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Must be in the set {%s}.\n"
msgstr ""
"%s: Неверное значение аргумента #%d: Должно быть из множества {%s}.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Array of floating point numbers "
"expected.\n"
msgstr ""
"%s: Неверный тип аргумента #%d: Ожидался массив чисел с плавающей точкой.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A scalar expected.\n"
msgstr "%s: Неверный размер аргумента #%d: Ожидался скаляр.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A polynomial expected.\n"
msgstr "%s: Неверный размер аргумента #%d: Ожидался полином.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: degree must be in the set {%s}.\n"
msgstr ""
"%s: Неверные значения аргумента #%d: степень должна принадлежать множеству "
"{%s}.\n"

#, c-format
msgid "%s: Incompatible input arguments #%d and #%d: Same degree expected.\n"
msgstr "%s: Несовместимые аргументы #%d и #%d: Степени должны совпадать.\n"

#, c-format
msgid "%s: Input argument #%d must be real.\n"
msgstr "%s: Аргумент #%d должен быть действительным числом.\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: Elements must be positive.\n"
msgstr ""
"%s: Неверные значения аргумента #%d: Элементы должны быть положительными.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Distance between Max and Min "
"elements must be greater than %g.\n"
msgstr ""
"%s: Неверные значения аргумента #%d: Расстояния между минимальным и "
"максимальным элементами должно быть больше %g.\n"

#, c-format
msgid ""
"%s: Incompatible input arguments #%d and #%d: Same numbers of elements "
"expected.\n"
msgstr ""
"%s: Несовместимые аргументы #%d и #%d: Должно быть одинаковое количество "
"элементов.\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: Not sampled high enough.\n"
msgstr ""
"%s: Неверные значения аргумента #%d: Недостаточная частота отсчётов.\n"

#, c-format
msgid "%s: Arguments #%d and #%d must have the same sizes.\n"
msgstr ""

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr "%s: Неверное количество аргументов: Ожидалось от %d до %d.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d or %d expected.\n"
msgstr "%s: Неверное количество аргументов: Ожидалось %d или %d.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A single input, single output system "
"expected.\n"
msgstr ""
"%s: Неверный размер аргумента #%d: Ожидалась система с одним входом и одним "
"выходом.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d:  Linear dynamical system or row "
"vector of floats expected.\n"
msgstr ""
"%s: Неверный тип аргумента #%d:  Ожидалась линейная динамическая система или "
"вектор-строка чисел с плавающей точкой.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Non-negative integers expected.\n"
msgstr ""
"%s: Неверные значения аргумента #%d: Ожидались неотрицательные целые числа.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Elements must be in the interval "
"[%s, %s].\n"
msgstr ""
"%s: Неверные значения аргумента #%d: Элементы должны быть в интервале [%s, "
"%s].\n"

#, c-format
msgid "%s: Wrong number of input argument(s): %d expected.\n"
msgstr "%s: Неверное количество аргументов: Ожидалось %d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A tall matrix expected.\n"
msgstr ""
"%s: Неверный размер аргумента #%d: Ожидалась высокая матрица (tall matrix).\n"

#, c-format
msgid ""
"%s: Incompatible input arguments #%d and #%d: Argument #%d expected to be "
"less than argument #%d.\n"
msgstr ""
"%s: Несовместимые аргументы #%d и #%d: Аргумент #%d должен быть меньше "
"аргумента #%d.\n"

#, c-format
msgid "%s: Incompatible input arguments #%d and #%d: Same sizes expected.\n"
msgstr ""
"%s: Несовместимые аргументы #%d и #%d: Они должны иметь одинаковые размеры.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: rational fraction array expected.\n"
msgstr ""
"%s: Неверный тип аргумента #%d: Ожидался массив рациональных функций.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A %d elements array expected.\n"
msgstr "%s: Неверный размер аргумента #%d: Ожидалось %d элементов массива.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: The first element must be %s and "
"the last %s.\n"
msgstr ""
"%s: Неверные значения аргумента #%d: Первый элементы должен быть %s, а "
"последний %s.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Elements must be in increasing "
"order.\n"
msgstr ""
"%s: Неверные значения аргумента #%d: Элементы должны быть отсортированы по "
"возрастанию\n"

#, c-format
msgid "%s: Too abrupt change near end of frequency range.\n"
msgstr ""
"%s: Слишком неожиданное изменение около границы частотного диапазона.\n"
