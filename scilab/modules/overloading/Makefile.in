@NEED_JAVA_TRUE@	if test -z "$(USEANT)"; then USEANT=0; else USEANT=1; fi; \
@NEED_JAVA_TRUE@	if test -f build.xml -a $$USEANT -eq 1; then \
@NEED_JAVA_TRUE@		$(ANT); \
@NEED_JAVA_TRUE@	fi

