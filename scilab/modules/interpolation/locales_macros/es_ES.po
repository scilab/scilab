#, c-format
msgid "%s: Wrong number of input arguments: Must be between %d and %d.\n"
msgstr ""
"%s: Número incorrecto de argumentos de entrada: Deben ser entre %d y %d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Two rows expected.\n"
msgstr ""
"%s: Tamaño incorrecto del argumento de entrada #%d: Se esperaban dos filas.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Vector expected.\n"
msgstr ""
"%s: Tamaño incorrecto del argumento de entrada #%d: Se esperaba un vector.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Vector or matrix expected.\n"
msgstr ""
"%s: Tamaño incorrecto del argumento de entrada #%d: Se esperaba un vector o "
"una matriz.\n"

#, c-format
msgid "%s: Wrong size for input arguments #%d and #%d: Same size expected.\n"
msgstr ""
"%s: Tamaño incorrecto de los argumentos de entrada #%d y #%d: Se los "
"esperaba del mismo tamaño.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Array of reals expected.\n"
msgstr ""
"%s: Tipo incorrecto de argumento de entrada #%d: Se esperaba un arreglo de "
"reales.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: ''%s'' or ''%s'' or ''%s'' "
"expected.\n"
msgstr ""
"%s: Valor incorrecto del argumento de entrada #%d: Se esperaba ''%s'' o "
"''%s'' o ''%s''.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: ''%s'' or real expected.\n"
msgstr ""
"%s: Valor incorrecto del argumento de entrada #%d: Se esperaba ''%s'' o un "
"real.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Reals expected but some NaN found.\n"
msgstr ""
"%s: Valor incorrecto del argumento de entrada #%d: Se esperaba un real, pero "
"se encontró NaN.\n"
