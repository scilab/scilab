  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  case $ANT in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_ANT="$ANT" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $ANTPATH/bin $ANTPATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_ANT="$as_dir/$ac_word$ac_exec_ext"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done
IFS=$as_save_IFS

  ;;
esac
fi
ANT=$ac_cv_path_ANT
if test -n "$ANT"; then
  { echo "$as_me:$LINENO: result: $ANT" >&5
echo "${ECHO_T}$ANT" >&6; }
else
  { echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6; }
fi


    fi

    # Check to see if ANT could not be found


    if test "x$ANT" = "x" ; then
        { { echo "$as_me:$LINENO: error: Cannot find ant" >&5
