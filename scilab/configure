    else
        echo "configure: failed program was:" >&5
        cat conftestSharedChecker.java >&5
        echo "configure: CLASSPATH was $CLASSPATH" >&5
    fi




    { $as_echo "$as_me:${as_lineno-$LINENO}: checking jgraphx" >&5
$as_echo_n "checking jgraphx... " >&6; }
