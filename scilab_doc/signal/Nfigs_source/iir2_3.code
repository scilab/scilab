xinit('iir2.ps')
dess(31)=1;
[pols,gn]=zpch1(3,.22942,2);
hs=gn/real(poly(pols,'s'));
fr=0:.05:3*%pi;
hsm=abs(freq(hs(2),hs(3),%i*fr));
plot(fr,hsm)
xend(),
xinit('iir3.ps')
z=poly(0,'z');
hz=horner(hs,2*(z-1)/(z+1));
fr=0:.005:.5;
hzm=abs(freq(hz(2),hz(3),exp(2*%pi*%i*fr)));
plot(fr,hzm)
    
xend(),

          
